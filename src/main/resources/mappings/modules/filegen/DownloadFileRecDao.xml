<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weichai.modules.filegen.dao.DownloadFileRecDao">


	<insert id="insertDownloadFileRec" parameterType="DownloadFileRec">
		INSERT
		INTO DOWNLOAD_FILE_REC(ID,LOGIN_NAME,PWD,SUPPLIER_TYPE,FILE_PATH,COUNT,CREATE_DATE,ADDRESS)
		VALUES
		(
			#{id},
			#{loginName},
			#{pwd},
			#{supplierType},
			#{filePath},
			#{count},
			SYSDATE,
			#{address}
		)
	</insert>

</mapper>

